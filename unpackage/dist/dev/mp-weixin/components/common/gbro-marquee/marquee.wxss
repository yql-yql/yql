.marquee_container {
	position: relative;
	width: 100%;
	height: 60rpx;
}
.marquee_text_left {
	display: flex;
	white-space: nowrap;
	-webkit-animation-name: around_left;
	        animation-name: around_left;
	-webkit-animation-duration: var(--speed--);
	        animation-duration: var(--speed--);
	-webkit-animation-iteration-count: infinite;
	        animation-iteration-count: infinite;
	-webkit-animation-timing-function: linear;
	        animation-timing-function: linear;
	line-height: 60rpx;
	position: absolute;
}
@-webkit-keyframes around_left {
from {
		left: 100%;
}
to {
		left: var(--marqueeWidth--);
}
}
@keyframes around_left {
from {
		left: 100%;
}
to {
		left: var(--marqueeWidth--);
}
}
.marquee_text_right {
	display: flex;
	white-space: nowrap;
	-webkit-animation-name: around_right;
